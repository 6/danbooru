<div id="c-wiki-pages">
  <div id="a-show">
    <%= render "sidebar" %>
    
    <section id="content">
      <h1 id="wiki-page-title"><%= @wiki_page.pretty_title %></h1>
      
      <div class="wiki-page-body">
        <%= format_text(@wiki_page.body) %>
      </div>
      
      <div id="wiki-page-posts">
        <%= @wiki_page.post_set.presenter.post_previews_html %>
      </div>
    </section>
  </div>
</div>

<%= render "secondary_links" %>